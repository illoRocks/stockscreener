{
  "python.pythonPath": "/usr/bin/python3",
  "files.exclude": {
    "**/*.pyc": true,
    "**/__pycache__": true
  },
  "editor.glyphMargin": true,
  "gitlens.codeLens.enabled": false,
  "[markdown]": {
    "editor.tabSize": 2
  },
  "python.autoComplete.addBrackets": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  }
}